<?xml version="1.0" encoding="utf-8"?>
<!-- Hello, DSO! - versie 27 mei 2019 -->
<Modules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://standaarden.overheid.nl/stop/DSO-PI10/manifest-ow.xsd manifest-ow.xsd"
         xmlns="https://standaarden.overheid.nl/stop/DSO-PI10">
    <!--
    Het domein element kan alleen omgevingswet zijn.
    Nodig om de verwerking in de keten beter te laten verlopen.
    -->
    <domein>omgevingswet</domein>
    <!-- Voor elke "imop:wordt" versie die in een besluit staat: -->
    <RegelingVersie>
        <!-- Verwijzing naar de versie van de regeling waar de modules bijhoren -->
        <FRBRwork>/akn/nl/act/gm0513/2018/omgevingsplan-Hello_DSO</FRBRwork>
        <FRBRExpression>/akn/nl/act/gm0513/2018/omgevingsplan-Hello_DSO/nld@2;2018-12-16</FRBRExpression>
        <!--
        Lijst met namen van bestanden die de CIM-OW data behorend bij
        de regelingversie bevatten. De bestanden moeten ook in
        manifest.xml voorkomen. Het objecttype moet zijn conform CIM-OW
        specificatie.
        -->
        <Bestand>
            <naam>Activiteiten.xml</naam>
            <objecttype>Activiteit</objecttype>
        </Bestand>
        <Bestand>
            <naam>Locaties.xml</naam>
            <objecttype>Gebied</objecttype>
        </Bestand>
        <Bestand>
            <naam>Regelteksten.xml</naam>
            <objecttype>Regeltekst</objecttype>
        </Bestand>
		
    </RegelingVersie>
</Modules>
